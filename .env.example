# Example .env - DO NOT use real keys here. Fill with your project's keys locally.

PROJECT_ID=your-project-id

# Web
WEB_API_KEY=your-web-api-key
WEB_APP_ID=your-web-app-id
WEB_MESSAGING_SENDER_ID=your-messaging-sender-id
WEB_AUTH_DOMAIN=your-auth-domain
WEB_STORAGE_BUCKET=your-storage-bucket
WEB_MEASUREMENT_ID=your-measurement-id

# Android
ANDROID_API_KEY=your-android-api-key
ANDROID_APP_ID=your-android-app-id
ANDROID_MESSAGING_SENDER_ID=your-messaging-sender-id
ANDROID_STORAGE_BUCKET=your-storage-bucket

# Windows
WINDOWS_API_KEY=your-windows-api-key
WINDOWS_APP_ID=your-windows-app-id
WINDOWS_MESSAGING_SENDER_ID=your-messaging-sender-id
WINDOWS_AUTH_DOMAIN=your-auth-domain
WINDOWS_STORAGE_BUCKET=your-storage-bucket
WINDOWS_MEASUREMENT_ID=your-measurement-id
